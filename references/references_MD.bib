	journal = {PLoS One},
	journal-full = {PloS one},
	mesh = {Adenocarcinoma; Antineoplastic Combined Chemotherapy Protocols; Cisplatin; Cluster Analysis; Drug Resistance, Neoplasm; Female; Fluorouracil; Gene Expression Profiling; Gene Expression Regulation, Neoplastic; High-Throughput Screening Assays; Humans; Male; Middle Aged; Neoplasm Metastasis; Stomach Neoplasms},
	month = {Feb},
	number = {2},
	pages = {e16694},
	pmc = {PMC3041770},
	pmid = {21364753},
	pst = {epublish},
	title = {A gene expression signature of acquired chemoresistance to cisplatin and fluorouracil combination chemotherapy in gastric cancer patients},
