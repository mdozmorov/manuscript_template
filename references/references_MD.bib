	journal-full = {Scientific reports},
	mesh = {Breast Neoplasms; Cell Line, Tumor; Chromosomes, Human; Down-Regulation; Female; Gene Expression Profiling; Gene Expression Regulation, Neoplastic; Genome, Human; Humans; Phenotype; Reproducibility of Results; Sequence Analysis, RNA; Signal Transduction},
	month = {05},
