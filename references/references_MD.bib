	mesh = {Animals; Base Sequence; Cell Cycle Proteins; Cells, Cultured; Chondroitin Sulfate Proteoglycans; Chromosomal Proteins, Non-Histone; Colonic Polyps; Cytoskeletal Proteins; DNA; Humans; Immunohistochemistry; Mice; Molecular Sequence Data; Promoter Regions, Genetic; TCF Transcription Factors; Trans-Activators; Transcription Factor 7-Like 2 Protein; Transcription Factors; Transcriptional Activation; beta Catenin},
	month = {May},
	number = {22},
