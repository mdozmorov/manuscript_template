	mesh = {Algorithms; Animals; Caenorhabditis elegans; Electronics; Escherichia coli; Food Chain; Gene Expression Regulation; Internet; Nerve Net; Neurons; Saccharomyces cerevisiae; Synapses},
	month = {Oct},
	number = {5594},
	pages = {824-7},
	pmid = {12399590},
	pst = {ppublish},
	title = {Network motifs: simple building blocks of complex networks},
	volume = {298},
