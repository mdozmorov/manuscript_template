	month = {08},
	number = {8},
	pages = {1263-1271},
	pmid = {31358994},
	pst = {ppublish},
	title = {Functional dissection of the Sox9-Kcnj2 locus identifies nonessential and instructive roles of TAD architecture},
	volume = {51},
