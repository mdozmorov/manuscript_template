	month = {May},
	number = {3},
	pages = {209-22},
	pmid = {8725772},
	pst = {ppublish},
	title = {An analysis of statistical term strength and its use in the indexing and retrieval of molecular biology texts},
	volume = {26},
	year = {1996}}

