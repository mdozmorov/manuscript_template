	mesh = {Cell Nucleolus; Cell Nucleus; Centromere; Chromosome Breakpoints; Chromosome Positioning; Chromosomes, Fungal; DNA Replication; Genome, Fungal; Haploidy; Imaging, Three-Dimensional; Intranuclear Space; RNA, Transfer; Replication Origin; Saccharomyces cerevisiae},
	month = {May},
	number = {7296},
	pages = {363-7},
