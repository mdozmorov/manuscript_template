	journal = {Nucleus},
	journal-full = {Nucleus (Austin, Tex.)},
	keywords = {3D genome; CTCF; FRAP; chromatin loops; cohesin; dynamics; loop extrusion; modeling; single-molecule imaging; topological domains},
	mesh = {Animals; Chromatin; Chromatin Assembly and Disassembly; Humans; Models, Molecular},
	month = {01},
