	mesh = {Cell Line, Tumor; Chromatin; Epithelial-Mesenchymal Transition; Female; Homeodomain Proteins; Humans; Recombinant Proteins; Repressor Proteins; Signal Transduction; Transcription Factor AP-1; Transfection; Triple Negative Breast Neoplasms; Tumor Necrosis Factor-alpha; Zinc Finger E-box Binding Homeobox 2},
	month = {Apr},
