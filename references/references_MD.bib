	keywords = {Hi-C; TAD; chromatin structure; compartment; embryo development; epigenetic; gamete; zygotic genome activation},
	mesh = {Animals; Chromatin; CpG Islands; DNA Methylation; DNA Replication; Embryo, Mammalian; Embryonic Development; Epigenesis, Genetic; Female; Germ Cells; Male; Metaphase; Mice; Mice, Inbred C57BL; Mice, Inbred DBA; Oocytes; Spermatozoa; Zygote},
	month = {Jul},
