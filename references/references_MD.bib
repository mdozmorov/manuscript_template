	mesh = {Antigens, Neoplasm; Biomarkers; Breast; Cell Adhesion Molecules; Cell Culture Techniques; Cell Differentiation; Epithelial Cell Adhesion Molecule; Epithelial Cells; Female; Flow Cytometry; Gene Expression Regulation; Humans; Integrins; Mucin-1; Stem Cells},
	month = {May},
	number = {2},
	pages = {93-109},
