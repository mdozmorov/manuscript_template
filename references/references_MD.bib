	journal = {Nature},
	journal-full = {Nature},
	mesh = {Chromatin; DNA; Deoxyribonuclease I; Gene Expression Regulation; Genes; Genome, Human; Humans; Molecular Sequence Annotation; Promoter Regions, Genetic; Regulatory Sequences, Nucleic Acid},
