	doi = {10.1038/sj.onc.1208561},
	journal = {Oncogene},
	journal-full = {Oncogene},
	mesh = {Apocrine Glands; Biopsy; Breast Neoplasms; Female; Humans; Oligonucleotide Array Sequence Analysis; Phenotype; Receptor, ErbB-2; Receptors, Androgen; Receptors, Estrogen; Signal Transduction},
	month = {Jul},
	number = {29},
