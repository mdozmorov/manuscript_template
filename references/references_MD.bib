	keywords = {DiGeorge syndrome; chromosome conformation capture; genome organization; long-range interactions; schizophrenia},
	mesh = {B-Lymphocytes; Catechol O-Methyltransferase; Cell Line; Chromosome Deletion; Chromosome Mapping; Cluster Analysis; DiGeorge Syndrome; Fibroblasts; Gene Expression; Genetic Variation; Humans; Phenotype; Transcriptome},
	number = {6},
