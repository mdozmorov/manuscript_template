	doi = {10.1371/journal.pcbi.1000353},
	journal = {PLoS Comput Biol},
	journal-full = {PLoS computational biology},
	mesh = {Algorithms; Comorbidity; Disease Susceptibility; Epidemiologic Measurements; Humans; Phenotype; Proportional Hazards Models; Risk Assessment; Risk Factors; United States},
	month = {Apr},
	number = {4},
	pages = {e1000353},
	pmc = {PMC2661364},
