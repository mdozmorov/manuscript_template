	journal = {Genome Biol},
	journal-full = {Genome biology},
	keywords = {3D chromatin architecture; CTCF; CTCFL; Cancer; Chromatin insulation; Cohesin; Gene regulation; Loop extrusion},
	mesh = {Animals; CCCTC-Binding Factor; Chromatin Assembly and Disassembly; DNA-Binding Proteins; Embryonic Stem Cells; Female; Gene Expression Regulation, Neoplastic; Humans; Male; Mice},
