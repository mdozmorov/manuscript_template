	keywords = {Cancer; Copy-number; Hi-C; Normalization},
	mesh = {Chromosome Aberrations; Chromosome Mapping; Computational Biology; DNA Copy Number Variations; Genome, Human; Genomics; Humans; Neoplasms},
	month = {Sep},
	number = {1},
	pages = {313},
	pmc = {PMC6127909},
	pmid = {30189838},
	pst = {epublish},
	title = {Effective normalization for copy number variation in Hi-C data},
	volume = {19},
