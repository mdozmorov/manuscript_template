	journal = {Nature},
	journal-full = {Nature},
	mesh = {Animals; Cell Nucleus; Chromatin; Chromosomes; Genetic Techniques; Male; Mice; Models, Molecular; Molecular Conformation; Single-Cell Analysis; X Chromosome},
