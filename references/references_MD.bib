	journal = {Brief Bioinform},
	journal-full = {Briefings in bioinformatics},
	month = {Jun},
	pmid = {29939197},
	pst = {aheadofprint},
	title = {Disease classification: from phenotypic similarity to integrative genomics and beyond},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1093/bib/bby049}}

