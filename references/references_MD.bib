	pages = {e133},
	pmc = {PMC4787761},
	pmid = {26163063},
	pst = {ppublish},
	title = {Utilizing mapping targets of sequences underrepresented in the reference assembly to reduce false positive alignments},
	volume = {43},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1093/nar/gkv671}}

