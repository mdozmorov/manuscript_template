	journal = {Nature},
	journal-full = {Nature},
	mesh = {Alleles; Animals; Binding Sites; CCCTC-Binding Factor; Chromatin; Chromosomes, Mammalian; Embryonic Stem Cells; Female; Gene Silencing; Male; Mice; Neural Stem Cells; Promoter Regions, Genetic; RNA, Long Noncoding; Repressor Proteins; Sequence Analysis; Transcription, Genetic; X Chromosome; X Chromosome Inactivation},
	month = {07},
