	number = {13},
	pages = {i133-i141},
	pmc = {PMC6022626},
	pmid = {29949986},
	pst = {ppublish},
	title = {Predicting CTCF-mediated chromatin loops using CTCF-MP},
	volume = {34},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1093/bioinformatics/bty248}}

