	mesh = {Cell Cycle Proteins; Chromosomal Proteins, Non-Histone; DNA; DNA-Binding Proteins; HeLa Cells; Holoenzymes; Humans; Nucleic Acid Conformation; Proton-Translocating ATPases},
	month = {12},
	number = {6471},
