@article{Soumillon:2014aa,
	author = {Soumillon, Magali and Cacchiarelli, Davide and Semrau, Stefan and van Oudenaarden, Alexander and Mikkelsen, Tarjei S.},
	date = {2014/01/01},
	date-added = {2020-04-20 11:02:52 -0400},
