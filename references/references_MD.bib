	mesh = {Animals; Cell Differentiation; DNA, Intergenic; Embryonic Stem Cells; Epigenesis, Genetic; Epigenomics; Female; Fibroblasts; Gene Expression Regulation; Histones; In Situ Hybridization, Fluorescence; Male; Methylation; Mice; Molecular Sequence Data; Promoter Regions, Genetic; RNA, Long Noncoding; RNA, Untranslated; Transcriptome; X Chromosome; X Chromosome Inactivation},
	month = {Apr},
