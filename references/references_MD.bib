	mesh = {Colorectal Neoplasms; DNA Topoisomerases, Type II; Humans; Middle Aged; Oncogenes; Oxaliplatin; Poly-ADP-Ribose Binding Proteins; RNA Helicases; Trans-Activators},
	month = {05},
	number = {6},
	pages = {519},
	pmc = {PMC8140095},
	pmid = {34021129},
	pst = {epublish},
	title = {UPF1 promotes chemoresistance to oxaliplatin through regulation of TOP2A activity and maintenance of stemness in colorectal cancer},
