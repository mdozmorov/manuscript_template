	pmc = {PMC3459874},
	pmid = {23028288},
	pst = {ppublish},
	title = {Enhancing the prioritization of disease-causing genes through tissue specific protein interaction networks},
	volume = {8},
	year = {2012},
	bdsk-url-1 = {https://dx.doi.org/10.1371/journal.pcbi.1002690}}

