	journal = {Cell Rep},
	journal-full = {Cell reports},
	keywords = {3D genome organization; DNA methylation; allele-specific replication; cancer; chromatin; epigenome; replication timing; single-cell sequencing},
	month = {09},
	number = {12},
	pages = {109722},
