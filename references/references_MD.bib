	mesh = {Animals; Antigens, CD29; Biomarkers, Tumor; Female; Flow Cytometry; Genes, erbB-2; Genes, p53; Integrin beta3; Mammary Neoplasms, Experimental; Mice; Mice, Inbred BALB C; Mice, Transgenic; Neoplastic Stem Cells; Precancerous Conditions; Stem Cells; Wnt1 Protein},
	month = {Oct},
	number = {19},
	pages = {7711-7},
	pmid = {18829523},
	pst = {ppublish},
	title = {The mammary progenitor marker CD61/beta3 integrin identifies cancer stem cells in mouse models of mammary tumorigenesis},
	volume = {68},
