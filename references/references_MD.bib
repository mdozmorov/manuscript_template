	mesh = {Biomarkers, Tumor; Cell Cycle Checkpoints; Drug Resistance, Neoplasm; Female; Humans; Neoplasm Metastasis; Oligonucleotides, Antisense; Prognosis; RNA, Long Noncoding; Triple Negative Breast Neoplasms},
	month = {Feb},
