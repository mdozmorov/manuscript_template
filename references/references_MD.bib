	journal = {Bioinformatics},
	journal-full = {Bioinformatics (Oxford, England)},
	mesh = {Automation; Databases, Genetic; Genomics; Molecular Sequence Annotation; Oligonucleotide Array Sequence Analysis; Polymorphism, Single Nucleotide; Programming Languages; Software},
	month = {Feb},
	number = {3},
	pages = {419-20},
	pmc = {PMC3268239},
	pmid = {22155868},
	pst = {ppublish},
	title = {GenomeRunner: automating genome exploration},
	volume = {28},
