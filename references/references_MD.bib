	journal = {Clin Epigenetics},
	journal-full = {Clinical epigenetics},
	keywords = {3D spheroids; Hi-C; Looping genes; Organoids; Tamoxifen-resistant breast cancer},
	mesh = {Adult; Breast Neoplasms; Chromatin; DNA Methylation; Endocrine Disruptors; Female; Humans; Middle Aged; Spheroids, Cellular},
