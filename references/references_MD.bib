	journal = {Nucleic Acids Res},
	journal-full = {Nucleic acids research},
	mesh = {Algorithms; Animals; Chromatin; Drosophila melanogaster; Epigenesis, Genetic; Gene Expression Regulation; Genome; Humans; Nucleic Acid Conformation; Probability; Protein Folding; Reproducibility of Results; Software},
