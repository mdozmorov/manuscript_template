	keywords = {ETS1; GATA3; IL32; MYBL1; TMEM158},
	month = {Jul},
	number = {7-8},
	pages = {659-672},
	pmc = {PMC5620011},
	pmid = {28966727},
	pst = {ppublish},
	title = {Identification of candidate genes associated with triple negative breast cancer},
	volume = {8},
	year = {2017},
