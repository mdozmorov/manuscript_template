	doi = {10.1016/j.cell.2015.11.024},
	journal = {Cell},
	journal-full = {Cell},
	mesh = {Animals; Cell Cycle Proteins; Cell Line; Chromatin; Chromosomal Proteins, Non-Histone; Chromosomes; DNA Packaging; Genome, Human; Humans; RNA Polymerase II; Repressor Proteins; Salamandridae; Transcription, Genetic},
	month = {Dec},
	number = {7},
	pages = {1611-27},
	pmc = {PMC4734140},
	pmid = {26686651},
	pst = {ppublish},
