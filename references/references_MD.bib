	mesh = {Algorithms; Analysis of Variance; Base Composition; Biostatistics; Databases, Nucleic Acid; Gene Expression Profiling; High-Throughput Nucleotide Sequencing; Humans; Oligonucleotide Array Sequence Analysis; Sequence Analysis, RNA},
	month = {Apr},
	number = {2},
	pages = {204-16},
	pmc = {PMC3297825},
	pmid = {22285995},
	pst = {ppublish},
	title = {Removing technical variability in RNA-seq data using conditional quantile normalization},
	volume = {13},
	year = {2012},
	bdsk-url-1 = {http://dx.doi.org/10.1093/biostatistics/kxr054}}

