	mesh = {Biomass; Cell Line, Tumor; Drug Resistance, Neoplasm; Drug Screening Assays, Antitumor; Humans; Interferometry; Kinetics; Melanoma; Protein Kinase Inhibitors; Proto-Oncogene Proteins B-raf},
	month = {03},
	number = {5},
	pages = {3299-3306},
	pmid = {29381859},
	pst = {ppublish},
	title = {High-Speed Live-Cell Interferometry: A New Method for Quantifying Tumor Drug Resistance and Heterogeneity},
