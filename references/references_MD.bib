	journal = {Nature},
	journal-full = {Nature},
	mesh = {Alleles; Cell Line; Cell Line, Tumor; Clone Cells; DNA Methylation; Embryonic Stem Cells; Epigenesis, Genetic; Fibroblasts; Genome, Human; Humans; Induced Pluripotent Stem Cells},
	month = {Sep},
	number = {7516},
