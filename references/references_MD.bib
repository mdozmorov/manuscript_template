	keywords = {CpGs; DNA methylation; Illumina microarray; SNPs; cross-reactive probe; oligonucleotide probe; polymorphic CpG},
	mesh = {Adult; Chromosomes, Human, X; Chromosomes, Human, Y; CpG Islands; DNA Methylation; DNA Probes; Female; Genome, Human; Humans; Male; Oligonucleotide Array Sequence Analysis; Polymorphism, Single Nucleotide},
	month = {Feb},
