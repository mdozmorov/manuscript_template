	doi = {10.1073/pnas.1911708117},
	journal = {Proc Natl Acad Sci U S A},
	journal-full = {Proceedings of the National Academy of Sciences of the United States of America},
	keywords = {3D genome organization; BORIS; CTCF; cohesin},
	mesh = {Binding Sites; Breast Neoplasms; CCCTC-Binding Factor; Cell Cycle Proteins; Chromatin; Chromosomal Proteins, Non-Histone; DNA, Neoplasm; DNA-Binding Proteins; Female; Genome, Human; Humans; Protein Binding; Protein Domains; Tumor Cells, Cultured},
	month = {01},
	number = {4},
	pages = {2020-2031},
	pmc = {PMC6995019},
	pmid = {31937660},
