	doi = {10.1038/nature06758},
	journal = {Nature},
	journal-full = {Nature},
	mesh = {Adipose Tissue; Adolescent; Adult; Aged; Aged, 80 and over; Animals; Blood; Body Mass Index; Cohort Studies; European Continental Ancestry Group; Female; Gene Expression Profiling; Gene Expression Regulation; Genome, Human; Humans; Iceland; Lod Score; Male; Mice; Middle Aged; Obesity; Polymorphism, Single Nucleotide; Quantitative Trait Loci; Sample Size; Waist-Hip Ratio},
