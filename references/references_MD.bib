	date-modified = {2020-07-14 14:07:03 -0400},
	doi = {10.1186/s13059-019-1658-7},
	journal = {Genome Biol},
	journal-full = {Genome biology},
	mesh = {Genomics; High-Throughput Nucleotide Sequencing; Humans; Neoplasms; Quality Control; Reproducibility of Results; Software; Tumor Cells, Cultured},
	month = {03},
