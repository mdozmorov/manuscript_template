	keywords = {Chromosome interaction network; Human genome; Multi-scale community mining; Spectral graph wavelets; Structural domain hierarchical organisation},
	mesh = {Algorithms; Chromatin; Chromosomes, Human; Computational Biology; Genome, Human; High-Throughput Nucleotide Sequencing; Humans; Sequence Analysis, DNA},
	month = {Apr},
