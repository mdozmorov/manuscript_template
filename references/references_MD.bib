	keywords = {Breast cancer; RNA interference; hyaluronan; hyaluronan synthase 2; metastasis; tumorigenesis},
	mesh = {Blotting, Western; Breast Neoplasms; Carcinogenesis; Carcinoma, Ductal, Breast; Cell Line, Tumor; Enzyme-Linked Immunosorbent Assay; Extracellular Matrix; Female; Gene Knockdown Techniques; Glucuronosyltransferase; Humans; Hyaluronan Synthases; Hyaluronic Acid; Immunohistochemistry; Neoplasm Invasiveness; RNA, Small Interfering; Real-Time Polymerase Chain Reaction; Transfection; Tumor Microenvironment; Up-Regulation},
	number = {10},
