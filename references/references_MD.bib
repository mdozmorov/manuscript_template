	mesh = {Aneuploidy; Chromatin; Chromosome Aberrations; Clonal Evolution; DNA Copy Number Variations; DNA Damage; Genome, Human; Humans; Models, Theoretical; Neoplasms; Nucleic Acid Conformation},
