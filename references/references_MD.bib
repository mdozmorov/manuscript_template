	mesh = {Carboplatin; Cisplatin; DNA Repair; DNA-Binding Proteins; Endonucleases; Female; Humans; Ovarian Neoplasms; Proteins; RNA, Messenger; Xeroderma Pigmentosum Group A Protein},
	month = {Aug},
