	doi = {10.1186/s13059-016-0992-2},
	journal = {Genome Biol},
	journal-full = {Genome biology},
	keywords = {Capture Hi-C; Convolution background model; Gene regulation; Nuclear organisation; P value weighting; Promoter-enhancer interactions},
	number = {1},
