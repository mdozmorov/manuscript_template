	mesh = {Chromosomes, Human; Computational Biology; Databases, Genetic; Genome, Human; Genomics; Humans; Imaging, Three-Dimensional; In Situ Hybridization, Fluorescence; Models, Molecular; Nucleic Acid Conformation},
	month = {02},
	number = {3},
	pages = {1049-1058},
	pmc = {PMC5430849},
	pmid = {28180292},
	pst = {ppublish},
	title = {3D genome structure modeling by Lorentzian objective function},
	volume = {45},
	year = {2017},
