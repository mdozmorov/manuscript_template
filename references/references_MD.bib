	doi = {10.1038/ng.167},
	journal = {Nat Genet},
	journal-full = {Nature genetics},
	mesh = {Bayes Theorem; Cluster Analysis; Crosses, Genetic; Gene Expression Regulation, Fungal; Gene Regulatory Networks; Genome, Fungal; Genomics; Models, Statistical; Organisms, Genetically Modified; Quantitative Trait Loci; Saccharomyces cerevisiae; Transcription Factors},
	month = {Jul},
