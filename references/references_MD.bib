@article{Ohlsson:2010uy,
	abstract = {The multifunctional zinc-finger protein CCCTC-binding factor (CTCF) is a very strong candidate for the role of coordinating the expression level of coding sequences with their three-dimensional position in the nucleus, apparently responding to a "code" in the DNA itself. Dynamic interactions between chromatin fibers in the context of nuclear architecture have been implicated in various aspects of genome functions. However, the molecular basis of these interactions still remains elusive and is a subject of intense debate. Here we discuss the nature of CTCF-DNA interactions, the CTCF-binding specificity to its binding sites and the relationship between CTCF and chromatin, and we examine data linking CTCF with gene regulation in the three-dimensional nuclear space. We discuss why these features render CTCF a very strong candidate for the role and propose a unifying model, the "CTCF code," explaining the mechanistic basis of how the information encrypted in DNA may be interpreted by CTCF into diverse nuclear functions.},
	author = {Ohlsson, Rolf and Lobanenkov, Victor and Klenova, Elena},
	date-added = {2022-04-30 13:40:17 -0400},
	date-modified = {2022-04-30 13:40:17 -0400},
	doi = {10.1002/bies.200900118},
	journal = {Bioessays},
	journal-full = {BioEssays : news and reviews in molecular, cellular and developmental biology},
	mesh = {Animals; Base Sequence; Binding Sites; CCCTC-Binding Factor; Cell Cycle Proteins; Cell Nucleus; Chromatin; Chromosomal Proteins, Non-Histone; DNA; DNA-Binding Proteins; Epigenesis, Genetic; Gene Expression; Humans; Mice; Models, Biological; Models, Genetic; Nucleosomes; Protein Interaction Domains and Motifs; Repressor Proteins},
	month = {Jan},
	number = {1},
	pages = {37-50},
