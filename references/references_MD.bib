	doi = {10.1016/j.tig.2013.05.010},
	journal = {Trends Genet},
	journal-full = {Trends in genetics : TIG},
	keywords = {RNA-seq; gene expression patterns; housekeeping genes; internal control; next generation sequencing},
	mesh = {Databases, Genetic; Genes, Essential; High-Throughput Nucleotide Sequencing; Humans},
	month = {Oct},
