	date-modified = {2018-06-01 11:34:54 +0000},
	doi = {10.1126/science.aad6469},
	journal = {Science},
	journal-full = {Science (New York, N.Y.)},
	mesh = {Cerebral Cortex; Gene Expression Profiling; Gene Expression Regulation; Genetic Predisposition to Disease; Humans; Mental Disorders; Multifactorial Inheritance; Nervous System Diseases; Polymorphism, Single Nucleotide; Transcription, Genetic},
	month = {02},
	number = {6376},
	pages = {693-697},
	pmc = {PMC5898828},
	pmid = {29439242},
	pst = {ppublish},
	title = {Shared molecular neuropathology across major psychiatric disorders parallels polygenic overlap},
