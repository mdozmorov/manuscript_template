	doi = {10.1101/gr.136101.111},
	journal = {Genome Res},
	journal-full = {Genome research},
	mesh = {Binding Sites; CCCTC-Binding Factor; Cell Line; Chromatin Immunoprecipitation; Cluster Analysis; CpG Islands; DNA Methylation; Gene Expression Regulation; High-Throughput Nucleotide Sequencing; Humans; Repressor Proteins},
	month = {Sep},
	number = {9},
	pages = {1680-8},
	pmc = {PMC3431485},
	pmid = {22955980},
	pst = {ppublish},
	title = {Widespread plasticity in CTCF occupancy linked to DNA methylation},
	volume = {22},
