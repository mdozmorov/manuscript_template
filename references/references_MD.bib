	doi = {10.1038/nature23263},
	journal = {Nature},
	journal-full = {Nature},
	mesh = {Alleles; Animals; Blastocyst; Chromatin; Chromatin Assembly and Disassembly; Chromosomes, Mammalian; Embryonic Development; Female; Fertilization; Gene Expression Regulation, Developmental; Male; Mice; Transcription, Genetic; Zygote},
