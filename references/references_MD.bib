	doi = {10.1186/1479-7364-8-14},
	journal = {Hum Genomics},
	journal-full = {Human genomics},
	mesh = {Bipolar Disorder; Data Interpretation, Statistical; Exome; High-Throughput Nucleotide Sequencing; Humans; Polymorphism, Single Nucleotide; Sequence Analysis, DNA; Software},
	month = {Jul},
	pages = {14},
	pmc = {PMC4129436},
	pmid = {25078893},
	pst = {epublish},
	title = {Validation and assessment of variant calling pipelines for next-generation sequencing},
	volume = {8},
