	journal = {Semin Liver Dis},
	journal-full = {Seminars in liver disease},
	mesh = {Gene Frequency; Genetic Predisposition to Disease; Genetic Variation; Genome-Wide Association Study; Genomics; Heredity; Humans; Precision Medicine; Risk Assessment; Risk Factors},
	month = {May},
