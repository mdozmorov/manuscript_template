@article{Kerpedjiev:2017aa,
	author = {Kerpedjiev, Peter and Abdennur, Nezar and Lekschas, Fritz and McCallum, Chuck and Dinkla, Kasper and Strobelt, Hendrik and Luber, Jacob M. and Ouellette, Scott B. and Azhir, Alaleh and Kumar, Nikhil and Hwang, Jeewon and Lee, Soohyun and Alver, Burak H. and Pfister, Hanspeter and Mirny, Leonid A. and Park, Peter J. and Gehlenborg, Nils},
	date = {2017/01/01},
	date-added = {2018-04-28 15:19:08 +0000},
	date-modified = {2018-04-28 15:19:08 +0000},
	journal = {bioRxiv},
	m3 = {10.1101/121889},
	month = {01},
	n2 = {In the face of new multidimensional genomic data, visual tools are essential for exploring data, discovering patterns, and rationalizing and disseminating results. Within the field of genomics, this pursuit is often confounded by not only the size of the data but the variety of scales at which features are visible. Genome-wide interaction maps produced by Chromosome Conformation Capture techniques such as Hi-C are particularly challenging as patterns emerge at a range of scales from hundreds of kilobases to hundreds of megabases. Since automatic detection and classification of these features gives ambiguous results, visual exploration and comparison is essential for analysis of Hi-C data and its comparison with other genomic tracks, requiring navigation and comparison across multiple loci, multiple resolutions and multiple samples. Here we present HiGlass, an open source visualization tool built on web technologies that provides a rich interface for rapid, multiplex, and multiscale navigation of 2D genomic maps alongside 1D genomic tracks, allowing users to combine various data types, synchronize multiple visualization modalities, and effortlessly share fully customizable views with others. We demonstrate its utility in exploring different experimental conditions, comparing the results of analyses, and creating interactive snapshots to share with collaborators and the broader public. HiGlass is accessible online at http://higlass.io and is also available as a containerized application that can be run on any platform.},
	title = {HiGlass: Web-based Visual Exploration and Analysis of Genome Interaction Maps},
	ty = {JOUR},
	url = {http://biorxiv.org/content/early/2017/10/30/121889.abstract},
	year = {2017},
	bdsk-url-1 = {http://biorxiv.org/content/early/2017/10/30/121889.abstract}}

@article{Vidal:2018aa,
	abstract = {The three-dimensional conformation of genomes is an essential component of their biological activity. The advent of the Hi-C technology enabled an unprecedented progress in our understanding of genome structures. However, Hi-C is subject to systematic biases that can compromise downstream analyses. Several strategies have been proposed to remove those biases, but the issue of abnormal karyotypes received little attention. Many experiments are performed in cancer cell lines, which typically harbor large-scale copy number variations that create visible defects on the raw Hi-C maps. The consequences of these widespread artifacts on the normalized maps are mostly unexplored. We observed that current normalization methods are not robust to the presence of large-scale copy number variations, potentially obscuring biological differences and enhancing batch effects. To address this issue, we developed an alternative approach designed to take into account chromosomal abnormalities. The method, called OneD, increases reproducibility among replicates of Hi-C samples with abnormal karyotype, outperforming previous methods significantly. On normal karyotypes, OneD fared equally well as state-of-the-art methods, making it a safe choice for Hi-C normalization. OneD is fast and scales well in terms of computing resources for resolutions up to 5 kb.},
	author = {Vidal, Enrique and le Dily, Fran{\c c}ois and Quilez, Javier and Stadhouders, Ralph and Cuartero, Yasmina and Graf, Thomas and Marti-Renom, Marc A and Beato, Miguel and Filion, Guillaume J},
	date-added = {2018-04-06 19:46:40 +0000},
	date-modified = {2018-04-06 19:46:40 +0000},
	doi = {10.1093/nar/gky064},
	journal = {Nucleic Acids Res},
	journal-full = {Nucleic acids research},
	month = {Jan},
	pmid = {29394371},
	pst = {aheadofprint},
	title = {OneD: increasing reproducibility of Hi-C samples with abnormal karyotypes},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1093/nar/gky064}}

