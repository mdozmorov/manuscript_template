	doi = {10.1007/s10585-008-9193-z},
	journal = {Clin Exp Metastasis},
	journal-full = {Clinical \& experimental metastasis},
	mesh = {Adult; Aged; Animals; Antigens, CD11b; Antigens, CD45; Apoptosis; Brain Neoplasms; Breast Neoplasms; Cell Movement; Cell Proliferation; Female; Humans; Mice; Mice, Inbred BALB C; Middle Aged; Neuroglia},
	number = {7},
