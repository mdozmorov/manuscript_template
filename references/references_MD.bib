	mesh = {Animals; Breast Neoplasms; Cell Line, Tumor; Cell Movement; Cell Proliferation; Female; Gene Expression Regulation, Neoplastic; Humans; MAP Kinase Signaling System; MCF-7 Cells; Mammary Neoplasms, Experimental; Membrane Proteins; Mice; Mice, Nude; MicroRNAs; Neoplasm Metastasis; Nerve Tissue Proteins; Nifedipine; Verapamil},
	number = {12},
