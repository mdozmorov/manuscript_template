	doi = {10.3389/fcell.2021.716730},
	journal = {Front Cell Dev Biol},
	journal-full = {Frontiers in cell and developmental biology},
	keywords = {EIF2 signaling; RPL27A; ribosomal protein; single cell RNA-sequencing; triple negative breast cancer},
	pages = {716730},
	pmc = {PMC8419227},
	pmid = {34497807},
	pst = {epublish},
	title = {Ribosome Proteins Represented by RPL27A Mark the Development and Metastasis of Triple-Negative Breast Cancer in Mouse and Human},
