	keywords = {3D genome organization; ChIA-PET; biomarkers; cancer; chromatin interactions; translational medicine},
	mesh = {Chromatin; Genome, Human; Health; Humans; Models, Biological; Neoplasms; Precision Medicine; Translational Medical Research},
