	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Breast Neoplasms; Computer Simulation; Female; Gene Expression Profiling; Gene Expression Regulation, Neoplastic; Humans; Models, Genetic; Neoplasm Proteins; Neoplasms; Predictive Value of Tests; Prognosis; Reproducibility of Results; Treatment Outcome},
