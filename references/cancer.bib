	Keywords = {Adaptive biomarker; Assessing response; Cancer; Predictive biomarker; RECIST; Response Biomarker; Systemic therapy},
	Mesh = {Antineoplastic Agents; Biomarkers, Tumor; Humans; Neoplasms; Precision Medicine; Treatment Outcome},
	Month = {11},
	Number = {1},
	Pages = {850},
	Pmc = {PMC5097425},
	Pmid = {27814715},
	Pst = {epublish},
	Title = {Response biomarkers: re-envisioning the approach to tailoring drug therapy for cancer},
	Volume = {16},
	Year = {2016},
