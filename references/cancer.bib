	Doi = {10.1038/ng.3722},
	Journal = {Nat Genet},
	Journal-Full = {Nature genetics},
	Mesh = {DNA Copy Number Variations; Enhancer Elements, Genetic; Gene Expression Regulation, Neoplastic; Genetic Association Studies; Genetic Predisposition to Disease; Humans; Insulin Receptor Substrate Proteins; Insulin-Like Growth Factor II; Neoplasms; Promoter Regions, Genetic},
