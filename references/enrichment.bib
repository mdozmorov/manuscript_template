	Mesh = {Binding Sites; Cell Line, Tumor; Chromatin Immunoprecipitation; Enhancer Elements, Genetic; Gene Regulatory Networks; Genetic Loci; Genome, Human; Humans; Internet; MCF-7 Cells; Molecular Sequence Annotation; Regulatory Elements, Transcriptional; Sequence Analysis, DNA; Transcription Factors},
	Month = {Feb},
