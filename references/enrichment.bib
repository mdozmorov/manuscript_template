	Keywords = {3D genome; Assortativity; Chromatin Interaction Network; Embryonic stem cells; Enhancers; Epigenomics; Polycomb; Promoter Capture Hi-C; RNA polymerase},
	Month = {Jul},
	Number = {1},
	Pages = {152},
