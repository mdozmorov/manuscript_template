	Keywords = {ChIP-seq; Functional data analysis; H1; H2A.Z; H3K36me3; H3K4me3; H3K9ac; Histone modifications; Roadmap Epigenomics Consortium; Stem cells},
	Pages = {20},
	Pmc = {PMC4488123},
	Pmid = {26140054},
