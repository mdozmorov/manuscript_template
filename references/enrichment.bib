	Journal-Full = {Genomics},
	Mesh = {B-Lymphocytes; Cell Culture Techniques; CpG Islands; DNA Methylation; Epigenesis, Genetic; Gene Expression Profiling; Genome, Human; Herpesvirus 4, Human; Humans; Transformation, Genetic},
	Month = {Feb},
	Number = {2},
	Pages = {73-83},
