	Mesh = {Bayes Theorem; Cluster Analysis; Computational Biology; Databases, Genetic; Genome-Wide Association Study; Genomics; Humans; Models, Theoretical; Phenotype; Polymorphism, Single Nucleotide; Quantitative Trait, Heritable; ROC Curve; Reproducibility of Results},
	Number = {5},
	Pages = {e98122},
	Pmc = {PMC4028284},
	Pmid = {24844982},
