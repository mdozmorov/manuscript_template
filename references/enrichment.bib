	Mesh = {Computational Biology; Gene Expression Profiling; Genetic Diseases, Inborn; Genetic Variation; Genome-Wide Association Study; Humans; NF-kappa B; Polymorphism, Single Nucleotide; Protein Binding; Systems Biology; Transcription Factors},
