	Date-Added = {2016-11-21 14:34:38 +0000},
	Date-Modified = {2016-11-21 14:34:38 +0000},
	Doi = {10.1016/j.celrep.2016.10.059},
	Journal = {Cell Rep},
	Journal-Full = {Cell reports},
	Keywords = {DNase I hypersensitive sites; bioinformatics; epigenetics; epigenome-wide association study; histone marks},
	Month = {Nov},
	Number = {8},
	Pages = {2137-2150},
	Pmid = {27851974},
