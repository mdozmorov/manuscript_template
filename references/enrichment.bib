	Doi = {10.1093/nar/gkt227},
	Journal = {Nucleic Acids Res},
	Journal-Full = {Nucleic acids research},
	Mesh = {Cell Line, Tumor; Chromatin; Chromosomes, Human; Data Interpretation, Statistical; Genome; Humans; Leukemia; Mutation; Nucleic Acid Conformation; Sequence Analysis, DNA},
	Month = {May},
	Number = {10},
	Pages = {5164-74},
	Pmc = {PMC3664813},
	Pmid = {23571755},
	Pst = {ppublish},
	Title = {Handling realistic assumptions in hypothesis testing of 3D co-localization of genomic elements},
	Volume = {41},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1093/nar/gkt227}}

@article{Paulsen:2014aa,
	Abstract = {UNLABELLED: Recently developed methods that couple next-generation sequencing with chromosome conformation capture-based techniques, such as Hi-C and ChIA-PET, allow for characterization of genome-wide chromatin 3D structure. Understanding the organization of chromatin in three dimensions is a crucial next step in the unraveling of global gene regulation, and methods for analyzing such data are needed. We have developed HiBrowse, a user-friendly web-tool consisting of a range of hypothesis-based and descriptive statistics, using realistic assumptions in null-models.
AVAILABILITY AND IMPLEMENTATION: HiBrowse is supported by all major browsers, and is freely available at http://hyperbrowser.uio.no/3d. Software is implemented in Python, and source code is available for download by following instructions on the main site.},
