	Journal = {Genome Biol},
	Journal-Full = {Genome biology},
	Mesh = {Cell Line; Chromatin; DNA Replication Timing; DNA-Binding Proteins; Databases, Genetic; Epigenesis, Genetic; Genetic Loci; Humans; K562 Cells; Models, Molecular; Multigene Family; Repressor Proteins},
	Month = {May},
	Pages = {110},
	Pmc = {PMC4443654},
	Pmid = {26013771},
	Pst = {epublish},
	Title = {Integrative modeling reveals the principles of multi-scale chromatin boundary formation in human nuclear organization},
	Volume = {16},
