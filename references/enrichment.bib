	Mesh = {Alleles; DNA-Binding Proteins; Enhancer Elements, Genetic; Gene Expression Regulation; Genetic Predisposition to Disease; HEK293 Cells; Haplotypes; Humans; Intracellular Signaling Peptides and Proteins; Lupus Erythematosus, Systemic; Matrix Attachment Region Binding Proteins; NF-kappa B; Nuclear Proteins; Polymorphism, Single Nucleotide; Promoter Regions, Genetic; Signal Transduction},
	Number = {9},
	Pages = {e1003750},
	Pmc = {PMC3764111},
	Pmid = {24039598},
