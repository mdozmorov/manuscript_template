	Mesh = {Algorithms; CD4-Positive T-Lymphocytes; Gene Regulatory Networks; Histones; Models, Theoretical},
	Number = {9},
	Pages = {e1003168},
	Pmc = {PMC3764007},
	Pmid = {24039558},
	Pst = {ppublish},
	Title = {Finding associations among histone modifications using sparse partial correlation networks},
